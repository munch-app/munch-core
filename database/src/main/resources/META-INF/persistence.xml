<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns="http://java.sun.com/xml/ns/persistence"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
    <persistence-unit name="defaultPersistenceUnit">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>

        <class>app.munch.model.Article</class>
        <class>app.munch.model.ArticlePlace</class>
        <class>app.munch.model.ArticleRevision</class>

        <class>app.munch.model.Publication</class>
        <class>app.munch.model.PublicationArticle</class>

        <class>app.munch.model.Account</class>
        <class>app.munch.model.Profile</class>
        <class>app.munch.model.ProfileLink</class>
        <class>app.munch.model.ProfileRestriction</class>

        <class>app.munch.model.Image</class>

        <class>app.munch.model.Place</class>
        <class>app.munch.model.PlaceImage</class>
        <class>app.munch.model.PlaceRevision</class>
        <class>app.munch.model.PlaceLocking</class>

        <class>app.munch.model.Tag</class>

        <class>app.munch.model.ProfileSocial</class>
        <class>app.munch.model.SocialObject</class>
        <class>app.munch.model.Mention</class>

        <class>app.munch.model.Affiliate</class>
        <class>app.munch.model.AffiliateBrand</class>
        <class>app.munch.model.PlaceAffiliate</class>

        <class>app.munch.model.ChangeGroup</class>
        <class>app.munch.model.WorkerGroup</class>
        <class>app.munch.model.WorkerTask</class>
    </persistence-unit>
</persistence>
